{
  "name": "frager",
  "license": "MIT",
  "version": "1.0.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkatou7209/frag.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "module": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "http",
    "client",
    "fetch",
    "request",
    "typescript",
    "api",
    "rest",
    "frager"
  ],
  "scripts": {
    "build": "tsc && tsc-alias && rolldown -c rolldown.config.ts -o ./dist/index.mjs",
    "clean": "rm -rf ./dist",
    "test": "vitest run",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "rolldown": "^1.0.0-beta.38",
    "tsc-alias": "^1.8.16",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
